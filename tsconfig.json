{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "esNext",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "shared-library": [
        "dist/shared-library"
      ],
      "shared-library/*": [
        "dist/shared-library/*"
      ],
      "super-grid": [
        "dist/super-grid"
      ],
      "super-grid/*": [
        "dist/super-grid/*"
      ],
      "@xva/super-grid": [
        "dist/xva/super-grid"
      ],
      "@xva/super-grid/*": [
        "dist/xva/super-grid/*"
      ],
      "@xva/drodpdown": [
        "dist/xva/drodpdown"
      ],
      "@xva/drodpdown/*": [
        "dist/xva/drodpdown/*"
      ]
    }
  }
}